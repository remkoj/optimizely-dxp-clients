  "devDependencies": {
    "@remkoj/optimizely-cms-react": "workspace:6.0.0-pre14",
    "@remkoj/optimizely-graph-client": "workspace:6.0.0-pre14",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "19.2.3",
    "next": "^16.0.2",
    "prop-types": "^15.8.1",
